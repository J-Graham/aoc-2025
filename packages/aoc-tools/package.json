{
  "name": "@aoc-starter/aoc-tools",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "aoc-2025": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/aoc-tools/dist",
          "main": "packages/aoc-tools/src/index.ts",
          "tsConfig": "packages/aoc-tools/tsconfig.lib.json",
          "rootDir": "packages/aoc-tools/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./packages/aoc-tools/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./packages/aoc-tools/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "22.0.4",
    "tslib": "^2.3.0"
  },
  "generators": "./generators.json",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "generators.json"
  ]
}
